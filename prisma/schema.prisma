generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Call {
  id Int @default(autoincrement()) @id
  call_sid String @unique
  caller String
  reciever String
  call_status String
  voicemail_link String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
